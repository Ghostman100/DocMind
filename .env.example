# Qdrant settings
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
COLLECTION_NAME=documents

# Embedding model (можно использовать русские модели)
# Варианты: intfloat/multilingual-e5-large, cointegrated/rubert-tiny2, sentence-transformers/paraphrase-multilingual-MiniLM-L12-v2
EMBEDDING_MODEL=intfloat/multilingual-e5-large

# Chunking strategy: paragraph или recursive
CHUNKING_STRATEGY=paragraph

# Настройки для recursive chunking
CHUNK_SIZE=500
CHUNK_OVERLAP=50

# API settings
API_HOST=0.0.0.0
API_PORT=8000
